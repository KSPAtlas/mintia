#include "<df>/dragonfruit.h"

#include "<inc>/OSCalls.h"

fn FPuts { fd s -- }
	while (s@ gb)
		s@ gb Putc
		1 s +=
	end
end

fn FPutc { fd c -- }
	c@ OSConsolePutCharacter drop
end

fn Putc { c -- }
	c@ OSConsolePutCharacter drop
end

fn Malloc { size -- ptr }
	"Malloc NYI\n" Printf
end

fn Free { ptr -- }
	"Free NYI\n" Printf
end

fn Calloc { size -- ptr }
	"Calloc NYI\n" Printf
end