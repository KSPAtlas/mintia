#include "<df>/dragonfruit.h"

#include "<inc>/HALConsole.h"

#include "<inc>/Kernel.h"

fn FPuts { fd s -- }
	while (s@ gb)
		s@ gb Putc
		1 s +=
	end
end

fn FPutc { fd c -- }
	c@ HALPutc
end

fn Putc { c -- }
	c@ HALPutc
end

fn Malloc { size -- ptr }
	"Malloc used in kernel code\n" KeCrash
end

fn Free { ptr -- }
	"Free used in kernel code\n" KeCrash
end

fn Calloc { size -- ptr }
	"Calloc used in kernel code\n" KeCrash
end