#include "<df>/dragonfruit.h"
#include "<inc>/HALLog.h"

#include "<inc>/HALInterrupt.h"

extern HALPlatformInterruptRegister { handler int -- }

extern HALPlatformInterruptUnregister { int -- }

fn HALInterruptRegister { handler int -- }
	handler@ int@ HALPlatformInterruptRegister
end

fn HALInterruptUnregister { int -- }
	int@ HALPlatformInterruptUnregister
end